{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Log",
        "Args": { "pathFormat": "log-{Date}.txt" }
      }
    ]
  },
    "AllowedHosts": "*"
  }
